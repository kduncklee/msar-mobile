{
  "expo": {
    "name": "Malibu SAR",
    "slug": "msar-mobile",
    "version": "1.5.5",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "org.malibusar.app",
      "buildNumber": "23",
      "icon": "./assets/icon.png",
      "googleServicesFile": "./GoogleService-Info.plist",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "com.apple.developer.usernotifications.critical-alerts": 1
      },
      "entitlements": {
        "com.apple.developer.usernotifications.critical-alerts": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-foreground.png",
        "backgroundColor": "#2B3038"
      },
      "package": "org.malibusar.app",
      "versionCode": 29,
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-firebase/app",
      "@react-native-firebase/messaging",
      [
        "expo-build-properties",
        {
          "android": {
            "extraMavenRepos": [
              "../../node_modules/@notifee/react-native/android/libs"
              ]
          },
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "sounds": [
            "assets/sounds/distortion_1_time.mp3",
            "assets/sounds/distortion_3_times.mp3",
            "assets/sounds/radio_1_time.mp3",
            "assets/sounds/radio_4_times.mp3",
            "assets/sounds/sweet_1_time.mp3",
            "assets/sounds/sweet_6_times.mp3",
            "assets/sounds/trumpets_1_time.mp3",
            "assets/sounds/trumpets_4_times.mp3",
            "assets/sounds/yucatan_1_time.mp3",
            "assets/sounds/yucatan_6_times.mp3"
          ]
        }
      ]
    ],
    "scheme": "msar",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f39a7bb9-2132-4fbb-abc6-c08a0daef74b"
      }
    }
  }
}
